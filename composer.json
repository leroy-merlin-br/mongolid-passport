{
    "name": "leroy-merlin-br/mongolid-passport",
    "description": "Mongolid Passport provides OAuth2 server support to Laravel and Mongolid.",
    "keywords": ["laravel", "passport", "oauth", "mongolid", "mongolid-laravel", "mongodb"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/leroy-merlin-br/mongolid-passport/issues",
        "source": "https://github.com/leroy-merlin-br/mongolid-passport"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "BoitatÃ¡",
            "email": "boitata@leroymerlin.com.br"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-mongodb": "*",
        "firebase/php-jwt": "~3.0|~4.0|~5.0",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/auth": "^5.8 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/console": "^5.8 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/container": "^5.8 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/contracts": "^5.8 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/database": "^5.8 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/encryption": "^5.8 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/http": "^5.8 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/support": "^5.8 || ^6.0 || ^7.0 || ^8.0",
        "league/oauth2-server": "^7.0",
        "leroy-merlin-br/mongolid": "dev-chore/legacy-record",
        "leroy-merlin-br/mongolid-laravel": "^3.0",
        "mongodb/mongodb": "^1.4",
        "phpseclib/phpseclib": "^2.0",
        "symfony/psr-http-message-bridge": "~1.0",
        "zendframework/zend-diactoros": "~1.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.4"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Passport\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.0-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Passport\\PassportServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "replace": {
        "laravel/passport": "7.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
