{
    "name": "leroy-merlin-br/mongolid-passport",
    "description": "Mongolid Passport provides OAuth2 server support to Laravel and Mongolid.",
    "keywords": ["laravel", "passport", "oauth", "mongolid", "mongolid-laravel", "mongodb"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/leroy-merlin-br/mongolid-passport/issues",
        "source": "https://github.com/leroy-merlin-br/mongolid-passport"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "BoitatÃ¡",
            "email": "boitata@leroymerlin.com.br"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-mongodb": "*",
        "mongodb/mongodb": "^1.4",
        "firebase/php-jwt": "~3.0|~4.0|~5.0",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/auth": "~5.4",
        "illuminate/console": "~5.4",
        "illuminate/container": "~5.4",
        "illuminate/contracts": "~5.4",
        "illuminate/encryption": "~5.4",
        "illuminate/http": "~5.4",
        "illuminate/support": "~5.4",
        "leroy-merlin-br/mongolid-laravel": "~2.0",
        "league/oauth2-server": "^6.0",
        "phpseclib/phpseclib": "^2.0",
        "symfony/psr-http-message-bridge": "~1.0",
        "zendframework/zend-diactoros": "~1.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Passport\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Passport\\PassportServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "replace": {
        "laravel/passport": "3.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
