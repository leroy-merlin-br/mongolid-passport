{
    "name": "leroy-merlin-br/mongolid-passport",
    "description": "Mongolid Passport provides OAuth2 server support to Laravel and Mongolid.",
    "keywords": ["laravel", "passport", "oauth", "mongolid", "mongolid-laravel", "mongodb"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/leroy-merlin-br/mongolid-passport/issues",
        "source": "https://github.com/leroy-merlin-br/mongolid-passport"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Boitat√°",
            "email": "boitata@leroymerlin.com.br"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "ext-mongodb": "^1.20.0",
        "firebase/php-jwt": "^6.4",
        "illuminate/auth": "^11.0|^12.0",
        "illuminate/console": "^11.0|^12.0",
        "illuminate/container": "^11.0|^12.0",
        "illuminate/contracts": "^11.0|^12.0",
        "illuminate/cookie": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/encryption": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "lcobucci/jwt": "^4.3|^5.0",
        "league/oauth2-server": "^8.5.3",
        "leroy-merlin-br/mongolid": "dev-chore/upgrade-to-laravel-12",
        "leroy-merlin-br/mongolid-laravel": "dev-chore/upgrade-to-laravel-12",
        "mongodb/mongodb": "^1.20.0",
        "nyholm/psr7": "^1.5",
        "phpseclib/phpseclib": "^2.0|^3.0",
        "symfony/console": "^6.0|^7.0",
        "symfony/psr-http-message-bridge": "^2.1|^6.0|^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^7.35|^8.14|^9.0|^10.0",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.3|^10.5|^11.5"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Passport\\": "src/",
            "Laravel\\Passport\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Passport\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\Passport\\PassportServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "replace": {
        "laravel/passport": "12.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
